{
  "name": "openclaw-electron",
  "version": "1.0.0",
  "description": "OpenClaw Electron 桌面应用",
  "main": "dist-electron/main.js",
  "author": "OpenClaw",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "tsc -p tsconfig.node.json && vite build",
    "build:electron": "tsc -p tsconfig.node.json",
    "preview": "vite preview",
    "prepare:node": "node scripts/prepare-node.js",
    "prepare:openclaw": "node scripts/prepare-openclaw.js",
    "build:installer": "node scripts/build-installer.js",
    "build:installer-unpacked": "cross-env UNPACKED_ONLY=1 node scripts/build-installer.js",
    "electron:dev": "npm run build:electron && electron .",
    "typecheck": "tsc --noEmit",
    "rebuild": "cross-env PYTHON=\"C:\\Users\\gotmo\\.conda\\envs\\node-gyp-env\\python.exe\" electron-builder install-app-deps"
  },
  "dependencies": {
    "@homebridge/node-pty-prebuilt-multiarch": "^0.13.1",
    "@xterm/addon-fit": "^0.10.0",
    "@xterm/xterm": "^5.5.0"
  },
  "devDependencies": {
    "@electron/rebuild": "^4.0.3",
    "@types/node": "^22.10.0",
    "@vitejs/plugin-react": "^4.3.4",
    "cross-env": "^7.0.3",
    "electron": "^33.2.1",
    "electron-builder": "^25.1.8",
    "typescript": "^5.6.3",
    "vite": "^6.0.5",
    "vite-plugin-electron": "^0.28.8",
    "vite-plugin-electron-renderer": "^0.14.6"
  }
}
